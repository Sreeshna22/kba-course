  for (let i = 0; i < b.length; i++) {
    for (let j = 0; j < b[i].length; j++) {
      b[i][j] = b[i][j] + b[i][j]; 
      document.write("b[" + i + "][" + j + "] = " + b[i][j] + "<br>");
    }
  }
   

    let a = [
      [1, 2],
      [3, 4]
    ];

    let b = [
      [5, 6],
      [7, 8]
    ];

    let sum = [];

    for (let i = 0; i < 2; i++) {
      sum[i] = [];
      for (let j = 0; j < 2; j++) {
        sum[i][j] = a[i][j] + b[i][j];
      }
    }

    // Show result
    document.write("Sum of matrices:<br>");
    document.write(sum[0] + "<br>");
    document.write(sum[1]);

    
